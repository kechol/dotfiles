{



    "window.zoomLevel": 0,
    "editor.tabSize": 2,
    "editor.renderIndentGuides": false,
    "editor.minimap.enabled": false,
    "editor.wordWrapColumn": 140,
    "editor.wordWrap": "on",
    "editor.formatOnSave": true,
    "workbench.sideBar.location": "right",
